<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://jakarta.ee/xml/ns/jakartaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://jakarta.ee/xml/ns/jakartaee http://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd"
         version="5.0">

    <!-- Définition de la servlet -->
     <servlet>
        <servlet-name>CreditServlet</servlet-name>
        <servlet-class>CreditServlet</servlet-class>
    </servlet>
    <!-- Mappage de la servlet -->
        <servlet-mapping>
